# Bug6102
SET sql_mode = 'NO_ENGINE_SUBSTITUTION';
create table t1 (date char(60), format char(60) not null);
insert into t1 values
    ('22-12-21', 'RR-MM-DD'),
    ('22-DEC-21', 'RR-MON-DD'),
    ('99-12-21', 'RR-MM-DD'),
    ('99-DEC-21', 'RR-MON-DD'),
    ('22-12-12','RRRR-MM-DD'),
    ('99-12-12','RRRR-MM-DD'),
    ('49','RR'),
    ('51','RR'),
    ('49','RRRR'),
    ('51','RRRR');

-- echo current year '2022'
    SET timestamp=UNIX_TIMESTAMP('2022-12-22 00:00:00');
SELECT date,format,to_timestamp(date, format) as to_timestamp from t1;
SELECT to_timestamp('221202','RRMMDD');
SELECT to_timestamp('22JAN02','RRMONDD');
SELECT to_timestamp('00','RR');
SELECT to_timestamp('01','RR');
SELECT to_timestamp('99','RR');

-- echo current year '1998'
set timestamp=UNIX_TIMESTAMP('1998-12-22 00:00:00');
SELECT date,format,to_timestamp(date, format) as to_timestamp from t1;
SELECT to_timestamp('221202','RRMMDD');
SELECT to_timestamp('22JAN02','RRMONDD');
SELECT to_timestamp('00','RR');
SELECT to_timestamp('01','RR');
SELECT to_timestamp('99','RR');

drop table t1;
