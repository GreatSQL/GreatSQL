create procedure p1()
begin
  declare n int default 0;
  repeat
    set n = n + 1;
  until (rawtohex('AB') = 'AB' or n >= 3) end repeat;
  select n;

  set n = 0;
  repeat
    set n = n + 1;
  until (rawtohex('AB') = '4142' or n >= 3) end repeat;
  select n;
end;|
call p1()|
call p1()|
drop procedure p1|

create procedure p1()
begin
  declare n int default 0;
  while (rawtohex('AB') = 'AB' and n < 3) do
    set n = n + 1;
  end while;
  select n;

  set n = 0;
  while (rawtohex('AB') = '4142' and n < 3) do
    set n = n + 1;
  end while;
  select n;
end;|
call p1()|
call p1()|
drop procedure p1|

create procedure p1()
begin
  declare n int default 0;
  label1: loop
    set n = n + 1;
    if (rawtohex('AB') = 'AB' and n < 3) then iterate label1; end if;
    leave label1;
  end loop label1;
  select n;

  set n = 0;
  label2: loop
    set n = n + 1;
    if (rawtohex('AB') = '4142' and n < 3) then iterate label2; end if;
    leave label2;
  end loop label2;
  select n;
end;|
call p1()|
call p1()|
drop procedure p1|

