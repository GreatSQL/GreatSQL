# ----------------------------------------------------------------------------
# Test for default parameters in procedure and function under sql_mode = oracle
# ----------------------------------------------------------------------------
# ----------------------------------------------------------------------------
# normal case
# ----------------------------------------------------------------------------
#----------------------------------------
# type:INT normal case
#----------------------------------------
#
# type:INT with default: NULL
#------------------------------------
SET sql_mode=ORACLE;

CREATE TABLE t1(c1 INT DEFAULT NULL);
INSERT INTO t1 VALUES();
SELECT HEX(c1), c1 FROM t1;

DELIMITER |;
CREATE PROCEDURE p1(a INT DEFAULT NULL)
AS BEGIN
  SELECT a;
END|

CREATE FUNCTION f1(a INT DEFAULT NULL) RETURN INT
AS BEGIN
  RETURN a;
END|

DELIMITER ;|

CALL p1(NULL);
CALL p1();
SELECT f1(NULL);
SELECT f1();
DROP PROCEDURE p1;
DROP FUNCTION f1;
DROP TABLE t1;
