create procedure p1() as
begin
select regexp_replace('111.222.3333', '([[:digit:]]{3})\.([[:digit:]]{3})\.([[:digit:]]{4})', '(\1) \2-\3') as regexp_replace from dual;
select regexp_replace('aaabbb ccc 123', 'aaa bbb', '*', 1, 0, 'x') as regexp_replace from dual;
end;//
call p1()//
regexp_replace
(111) 222-3333
regexp_replace
