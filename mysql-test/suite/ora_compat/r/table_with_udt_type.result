CREATE or replace PROCEDURE p1(min INT,max INT) as
CURSOR cur(pmin INT, pmax INT) is SELECT name1 FROM tt_air1 WHERE id BETWEEN pmin AND pmax;
va tt_air1.name1%type;
BEGIN
OPEN cur(min,max);
LOOP
FETCH cur INTO va;
EXIT WHEN cur%NOTFOUND;
select va;
END LOOP;
CLOSE cur;
END;
$$
call p1(1,10);
va
id:50 | type:name1
va
id:100 | type:aabb2
va
NULL
va
id:NULL | type:NULL
call p1(1,10);
va
id:50 | type:name1
va
id:100 | type:aabb2
va
NULL
va
id:NULL | type:NULL
drop PROCEDURE p1;
