CREATE OR REPLACE PROCEDURE p1() As
id int;
BEGIN
SELECT ratio_to_report(sidec) over (partition by sidea) INTO id FROM triangle1 where sidea=-1;
select id;
SELECT ratio_to_report(id) over (partition by id) from dual;
select id;
END;
