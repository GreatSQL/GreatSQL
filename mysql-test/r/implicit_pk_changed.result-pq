[# test new added unique key become pk, original implicit pk to unique key]
create table t1 (a int not null, b int not null, unique key(a));
show create table t1;
Table	Create Table
t1	CREATE TABLE `t1` (
  `a` int NOT NULL,
  `b` int NOT NULL,
  UNIQUE KEY `a` (`a`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci
insert into t1 values (1,11),(2,22),(3,33);
select * from t1;
a	b
1	11
2	22
3	33
alter table t1 add unique key(b), drop key a, add unique key(a), algorithm=inplace;
show create table t1;
Table	Create Table
t1	CREATE TABLE `t1` (
  `a` int NOT NULL,
  `b` int NOT NULL,
  UNIQUE KEY `b` (`b`),
  UNIQUE KEY `a` (`a`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci
select * from t1;
a	b
1	11
2	22
3	33

